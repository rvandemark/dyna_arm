#!/usr/bin/env python
# license removed for brevity
import rospy
from std_msgs.msg import UInt8

def talker():
    pub = rospy.Publisher('chatter', UInt8, queue_size=10)
    rospy.init_node('talker', anonymous=True)
    pub.publish(30)


if __name__ == '__main__':
    try:
        talker()
    except rospy.ROSInterruptException:
        pass
